UPDATE ACTR_EQUI AS target
FROM (
    SELECT CLE_ACTR, DT_VAL_DEB
    FROM ACTR_EQUI
    WHERE CLE_ACTR IN (12112, 22867, 10977, 19676)
      AND TOP_COURANT = 0
      AND CLE_EQ = 210620010
) AS source
SET target.DT_VAL_DEB = source.DT_VAL_DEB
WHERE target.CLE_ACTR = source.CLE_ACTR
  AND target.TOP_COURANT = 1
  AND target.CLE_EQ = 210620014;
