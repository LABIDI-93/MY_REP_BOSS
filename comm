COALESCE(
    CASE 
        WHEN LIMITE_ENG.CLE_PROD_ELMNT IN (6873, 4574, 6387, 6870, 6386, 6872) 
            AND LIMITE_ENG.MT_NOML_INIT IS NOT NULL THEN LIMITE_ENG.MT_NOML_INIT
        WHEN LIMITE_ENG.CLE_SRCE_FOURN_ENTRPT IN (41, 221) 
            THEN LIMITE_ENG.MT_NOML_INIT_EUR 
        ELSE LIMITE_ENG.MT_NOML_INIT 
    END, 
    PREST_PRET.MT_RECALC, 
    CBE.CBE_MT_NOML_ACTUAL
) / 1000
